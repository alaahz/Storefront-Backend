{
  "name": "storefront-backend-",
  "version": "1.0.0",
  "description": "shopping app backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npx tsc",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate drop",
    "migrate:reset": "db-migrate reset",
    "watch": "rimraf ./build && tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "test": "export ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate db:drop test",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write  \"src/**/*.ts\""
  },
  "author": "Alaa Alhazmi",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "supertest": "^6.3.1",
    "validator": "^13.7.0"
  }
}
